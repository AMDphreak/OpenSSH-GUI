<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:materialIcons="clr-namespace:Material.Icons.Avalonia;assembly=Material.Icons.Avalonia"
        xmlns:viewModels="clr-namespace:OpenSSH_GUI.ViewModels"
        xmlns:openSshGui="clr-namespace:OpenSSH_GUI"
        xmlns:converters="clr-namespace:OpenSSH_GUI.Converters"
        xmlns:sys="clr-namespace:System;assembly=mscorlib"
        mc:Ignorable="d" d:DesignWidth="1150" d:DesignHeight="450"
        x:Class="OpenSSH_GUI.Views.MainWindow"
        x:DataType="viewModels:MainWindowViewModel"
        Width="1150"
        Height="450"
        MinWidth="900"
        MinHeight="300"

        
        Title="{x:Static openSshGui:StringsAndTexts.MainWindowTitle}">
    <Window.Resources>
        <converters:PlatformIdConverter x:Key="PlatformIdConverter" />

    </Window.Resources>
    <Window.Styles>
        <!-- Header cell border styling - can be applied to any element -->
        <Style Selector="Border.HeaderCell">
            <Setter Property="Background" Value="Transparent" />
            <Setter Property="BorderBrush" Value="LightGray" />
            <Setter Property="BorderThickness" Value="0 0 1 1" />
            <Setter Property="CornerRadius" Value="0" />
            <Setter Property="Margin" Value="0" />
        </Style>
        
        <Style Selector="ToggleButton.Sorter:pressed">
            <Setter Property="RenderTransform" Value="{x:Null}" />
        </Style>
        <Style Selector="ToggleButton.Sorter">
            <Setter Property="Template">
                <ControlTemplate>
                    <Border Classes="HeaderCell">
                        <ContentPresenter Content="{TemplateBinding Content}"
                                          HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                          VerticalAlignment="{TemplateBinding VerticalContentAlignment}" />
                    </Border>
                </ControlTemplate>
            </Setter>
            <Setter Property="IsThreeState" Value="True" />
            <Setter Property="ClickMode" Value="Release" />
            <Setter Property="HorizontalAlignment" Value="Stretch" />
            <Setter Property="VerticalAlignment" Value="Stretch" />
            <Setter Property="Margin" Value="0" />
            <Setter Property="CornerRadius" Value="0" />
            <Setter Property="HorizontalContentAlignment" Value="Center" />
            <Setter Property="VerticalContentAlignment" Value="Center" />
            <Setter Property="Padding" Value="0 10" />
        </Style>
        
        <!-- First column header needs left border -->
        <Style Selector="ToggleButton.Sorter.FirstColumn">
            <Setter Property="Template">
                <ControlTemplate>
                    <Border BorderBrush="LightGray" BorderThickness="1 0 1 1"
                            Background="Transparent" CornerRadius="0" Margin="0">
                        <ContentPresenter Content="{TemplateBinding Content}"
                                          HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                                          VerticalAlignment="{TemplateBinding VerticalContentAlignment}" />
                    </Border>
                </ControlTemplate>
            </Setter>
        </Style>
        
        <!-- Button hover styles for outline buttons -->
        <Style Selector="Button.AddKeyButton:pointerover">
            <Setter Property="BorderBrush" Value="#006600" />
        </Style>
        <Style Selector="Button.AddKeyButton:pointerover materialIcons|MaterialIcon">
            <Setter Property="Foreground" Value="#006600" />
        </Style>
        
        <Style Selector="Button.EditKnownHostsButton:pointerover">
            <Setter Property="BorderBrush" Value="#004499" />
        </Style>
        <Style Selector="Button.EditKnownHostsButton:pointerover materialIcons|MaterialIcon">
            <Setter Property="Foreground" Value="#004499" />
        </Style>
        
        <Style Selector="Button.EditAuthorizedKeysButton:pointerover">
            <Setter Property="BorderBrush" Value="#006699" />
        </Style>
        <Style Selector="Button.EditAuthorizedKeysButton:pointerover materialIcons|MaterialIcon">
            <Setter Property="Foreground" Value="#006699" />
        </Style>
        
        <!-- Status panel button default styles -->
        <Style Selector="Button.StatusPanelButton">
            <Setter Property="Background" Value="{DynamicResource ThemeControlLowBrush}" />
        </Style>

        <!-- Status panel button hover styles -->
        <Style Selector="Button.StatusPanelButton:pointerover">
            <Setter Property="Background" Value="{DynamicResource ThemeControlHighBrush}" />
        </Style>
        
        <!-- Table row separator styling -->
        <Style Selector="Separator.TableRowSeparator">
            <Setter Property="Background" Value="LightGray" />
            <Setter Property="Height" Value="1" />
            <Setter Property="Margin" Value="10 8" />
            <Setter Property="Opacity" Value="0.6" />
        </Style>
    </Window.Styles>

    <Grid RowDefinitions="Auto 1* Auto" ColumnDefinitions="* * *" Margin="0" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">

        <!-- Key List -->

        <Grid Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Grid.RowSpan="2" 
              RowDefinitions="50 1*" 
              MinHeight="200"
              MinWidth="860"
              VerticalAlignment="Top"
              HorizontalAlignment="Stretch"
              Margin="0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="150" MinWidth="150" MaxWidth="150" />
                <ColumnDefinition Width="70" MinWidth="70" MaxWidth="70" />
                <ColumnDefinition Width="1*" MinWidth="200" />
                <ColumnDefinition Width="150" MinWidth="150" MaxWidth="150" />
                <ColumnDefinition Width="220" MinWidth="220" MaxWidth="220" />
            </Grid.ColumnDefinitions>
            
            <!-- Top border to reach window edge -->
            <Border Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="5" 
                    BorderBrush="LightGray" 
                    BorderThickness="0 1 0 0" 
                    VerticalAlignment="Top" 
                    Height="1" 
                    Margin="0" />

            <!-- Heading  -->
            <ToggleButton Grid.Row="0" Grid.Column="0" Classes="Sorter FirstColumn" IsChecked="{Binding KeyTypeSort}" 
                          Width="150" MinWidth="150" MaxWidth="150" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="0">
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Spacing="3" Margin="5">
                    <materialIcons:MaterialIcon Kind="KeyVariant" Margin="0" Padding="0" />
                    <Label Content="{x:Static openSshGui:StringsAndTexts.MainWindowDataGridColumnHeadingKeyTypeText}" />
                    <materialIcons:MaterialIcon Kind="{Binding KeyTypeSortDirectionIcon}" Margin="5 0" Padding="0" />
                </StackPanel>
            </ToggleButton>

            <Border Grid.Row="0" Grid.Column="1" Classes="HeaderCell"
                     HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Spacing="3" Margin="10">
                    <materialIcons:MaterialIcon Kind="Fingerprint" Margin="0" Padding="0" />
                </StackPanel>
            </Border>

            <ToggleButton Grid.Row="0" Grid.Column="2" Classes="Sorter" IsChecked="{Binding PathSort}" 
                          HorizontalAlignment="Stretch" VerticalAlignment="Stretch" MinWidth="200" Margin="0">
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Spacing="3" Margin="5">
                    <materialIcons:MaterialIcon Kind="FolderOutline" Margin="0" Padding="0" />
                    <Label
                        Content="{x:Static openSshGui:StringsAndTexts.MainWindowDataGridColumnHeadingPathText}" />
                    <materialIcons:MaterialIcon Kind="{Binding PathSortDirectionIcon}" Margin="5 0" Padding="0" />
                </StackPanel>
            </ToggleButton>

            <ToggleButton Grid.Row="0" Grid.Column="3" Classes="Sorter" IsChecked="{Binding CommentSort}" 
                          Width="150" MinWidth="150" MaxWidth="150" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="0">
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Spacing="3" Margin="5">
                    <materialIcons:MaterialIcon Kind="Comment" Margin="0" Padding="0" />
                    <TextPresenter
                        Text="{x:Static openSshGui:StringsAndTexts.MainWindowDataGridColumnHeadingCommentText}" />
                    <materialIcons:MaterialIcon Kind="{Binding CommentSortDirectionIcon}" Margin="5 0" Padding="0" />
                </StackPanel>
            </ToggleButton>

            <ToggleButton Grid.Row="0" Grid.Column="4" Classes="Sorter" IsEnabled="False" 
                          Width="220" MinWidth="220" MaxWidth="220" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="0">
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Spacing="3" Margin="5">
                    <materialIcons:MaterialIcon Kind="FileExportOutline" />
                    <Label
                        Content="{x:Static openSshGui:StringsAndTexts.MainWindowDataGridColumnHeadingExportDeleteText}" />
                </StackPanel>
            </ToggleButton>

            <ScrollViewer Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="5"
                          VerticalScrollBarVisibility="Auto"
                          HorizontalScrollBarVisibility="Disabled"
                          ClipToBounds="True">
                <ItemsControl ItemsSource="{Binding SshKeys}"
                              VerticalAlignment="Stretch"
                              HorizontalAlignment="Stretch"
                              Name="TheItemsGrid"
                              ItemTemplate="{DynamicResource SshKeyFullStyle}" />

            </ScrollViewer>
        </Grid>

        <!-- Bottom Controls -->

        <Grid Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="3" RowDefinitions="Auto" ColumnDefinitions="Auto * Auto" 
              MinHeight="50" VerticalAlignment="Bottom" HorizontalAlignment="Stretch"
              Background="{DynamicResource ThemeControlLowBrush}">

            <!-- Action Buttons Group -->
            <Grid Grid.Column="0" ColumnDefinitions="Auto Auto Auto" HorizontalAlignment="Left" VerticalAlignment="Center" Margin="10 5">
                <!-- Open Add Key Window -->
                <Button Grid.Column="0" Classes="AddKeyButton"
                        Command="{Binding OpenCreateKeyWindow}"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        HorizontalContentAlignment="Center"
                        VerticalContentAlignment="Center"
                        Background="{DynamicResource ThemeControlLowBrush}"
                        BorderBrush="#00AA00"
                        BorderThickness="2"
                        MinWidth="80" MinHeight="40"
                        Margin="0 0 5 0">
                    <ToolTip.Tip>
                        <Label Content="{x:Static openSshGui:StringsAndTexts.MainWindowAddKeyToolTip}" />
                    </ToolTip.Tip>
                    <materialIcons:MaterialIcon Kind="KeyAdd" Foreground="#00AA00" />
                </Button>

                <!-- Open Edit Known Host Window  -->
                <Button Grid.Column="1" Classes="EditKnownHostsButton"
                        Command="{Binding OpenEditKnownHostsWindow}"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        HorizontalContentAlignment="Center"
                        VerticalContentAlignment="Center"
                        Background="{DynamicResource ThemeControlLowBrush}"
                        BorderBrush="#0066CC"
                        BorderThickness="2"
                        MinWidth="80" MinHeight="40"
                        Margin="5 0">
                    <ToolTip.Tip>
                        <StackPanel>
                            <Label Content="{x:Static openSshGui:StringsAndTexts.MainWindowEditKnownHostsFileToolTip}" />
                            <Label Content="{x:Static openSshGui:StringsAndTexts.MainWindowIsNotRemoteTooltipText}"
                                   Foreground="OrangeRed"
                                   IsVisible="{Binding !ServerConnection.IsConnected}" />
                        </StackPanel>
                    </ToolTip.Tip>
                    <materialIcons:MaterialIcon Kind="FileAccountOutline" Foreground="#0066CC" />
                </Button>

                <!-- Open Edit Authorized Keys Window -->
                <Button Grid.Column="2" Classes="EditAuthorizedKeysButton"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Center"
                        HorizontalContentAlignment="Center"
                        VerticalContentAlignment="Center"
                        Background="{DynamicResource ThemeControlLowBrush}"
                        BorderBrush="#0088CC"
                        BorderThickness="2"
                        Command="{Binding OpenEditAuthorizedKeysWindow}"
                        MinWidth="80" MinHeight="40"
                        Margin="5 0 0 0">
                    <ToolTip.Tip>
                        <StackPanel>
                            <Label Content="{x:Static openSshGui:StringsAndTexts.MainWindowEditAuthorizedKeys}" />
                            <Label Content="{x:Static openSshGui:StringsAndTexts.MainWindowIsNotRemoteTooltipText}"
                                   Foreground="OrangeRed"
                                   IsVisible="{Binding !ServerConnection.IsConnected}" />
                        </StackPanel>
                    </ToolTip.Tip>
                    <materialIcons:MaterialIcon Kind="FileCogOutline" Foreground="#0088CC" />
                </Button>
            </Grid>

            <!-- Spacer -->
            <Border Grid.Column="1" />

            <!-- Status Panel -->
            <Border Grid.Column="2" HorizontalAlignment="Right" VerticalAlignment="Center" Margin="10 5"
                    Background="{DynamicResource ThemeControlMidBrush}" CornerRadius="5" MinHeight="50">
            <Grid ColumnDefinitions="Auto Auto Auto Auto" HorizontalAlignment="Right">

                <!-- Connection -->

                <Button Grid.Column="0" Classes="StatusPanelButton" HorizontalAlignment="Stretch" VerticalAlignment="Center"
                        Background="{DynamicResource ThemeControlLowBrush}" Margin="5" Padding="7" CornerRadius="5" MinWidth="50" MinHeight="40"
                        Name="ConnectionButton"
                        Click="ConnectionButton_Click">
                    <ToolTip.Tip>
                        <StackPanel>
                            <Grid ColumnDefinitions="* 2*" RowDefinitions="* * *"
                                  IsVisible="{Binding ServerConnection.IsConnected}">
                                <Label Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="2" HorizontalAlignment="Center"
                                       Content="{Binding ServerConnection.ConnectionString}" FontWeight="Bold" />
                                <Label Grid.Row="1" Grid.Column="0"
                                       Content="{x:Static openSshGui:StringsAndTexts.MainWindowConnectionTooltipServerOs}" />
                                <Label Grid.Row="1" Grid.Column="1"
                                       Content="{Binding ServerConnection.ServerOs, Converter={StaticResource PlatformIdConverter}}"
                                       FontWeight="Bold" />
                                <Label Grid.Row="2" Grid.Column="0"
                                       Content="{x:Static openSshGui:StringsAndTexts.MainWindowConnectionTooltipConnectedOn}" />
                                <Label Grid.Row="2" Grid.Column="1" Content="{Binding ServerConnection.ConnectionTime}" />
                            </Grid>
                            <Label
                                Content="{x:Static openSshGui:StringsAndTexts.MainWindowConnectionTooltipDisconnected}"
                                IsVisible="{Binding !ServerConnection.IsConnected}" />
                        </StackPanel>


                    </ToolTip.Tip>
                    <Button.ContextMenu>
                        <ContextMenu PlacementAnchor="Top" Placement="Top" WindowManagerAddShadowHint="True">
                            <MenuItem Header="{x:Static openSshGui:StringsAndTexts.MainWindowConnectMenuConnect}"
                                      IsEnabled="{Binding !ServerConnection.IsConnected}"
                                      Command="{Binding OpenConnectToServerWindow}">
                                <MenuItem.Icon>
                                    <materialIcons:MaterialIconExt Kind="LanConnect" />
                                </MenuItem.Icon>
                            </MenuItem>
                            <MenuItem Header="{x:Static openSshGui:StringsAndTexts.MainWindowConnectMenuDisconnect}"
                                      IsEnabled="{Binding ServerConnection.IsConnected}"
                                      Command="{Binding DisconnectServer}">
                                <MenuItem.Icon>
                                    <materialIcons:MaterialIconExt Kind="LanDisconnect" />
                                </MenuItem.Icon>
                            </MenuItem>
                        </ContextMenu>
                    </Button.ContextMenu>
                    <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
                        <StackPanel Orientation="Horizontal" IsVisible="{Binding ServerConnection.IsConnected}">
                            <materialIcons:MaterialIcon Kind="Signal" Foreground="Green" />
                        </StackPanel>
                        <StackPanel Orientation="Horizontal" IsVisible="{Binding !ServerConnection.IsConnected}">
                            <materialIcons:MaterialIcon Kind="SignalOff" Foreground="Red" />
                        </StackPanel>
                    </StackPanel>
                </Button>

                <!-- Item Count -->

                <Button Grid.Column="1" Classes="StatusPanelButton" Background="{DynamicResource ThemeControlLowBrush}" Margin="5" Padding="5" HorizontalAlignment="Stretch"
                        VerticalAlignment="Stretch" CornerRadius="5" MinWidth="50" MinHeight="40"
                        Name="ItemCountButton"
                        Click="ItemCountButton_Click"
                        Content="{Binding ItemsCount}"
                        HorizontalContentAlignment="Center"
                        VerticalContentAlignment="Center">
                    <ToolTip.Tip>
                        <StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <Label
                                    Content="{x:Static openSshGui:StringsAndTexts.MainWindowFoundKeyPairsCountLabelPart1}" />
                                <Label Content="{Binding KeysInSshDirectoryCount}" FontWeight="Bold" />
                                <Label
                                    Content="{x:Static openSshGui:StringsAndTexts.MainWindowFoundKeyPairsCountLabelPart2}" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal">
                                <Label
                                    Content="{x:Static openSshGui:StringsAndTexts.MainWindowFoundKeyPairsCountLabelPart1}" />
                                <Label Content="{Binding KeysInAlternateLocationsCount}" FontWeight="Bold" />
                                <Label
                                    Content="{x:Static openSshGui:StringsAndTexts.MainWindowFoundKeyPairsCountLabelPart3}" />
                            </StackPanel>
                        </StackPanel>
                    </ToolTip.Tip>
                </Button>

                <!-- Settings -->

                <Button Grid.Column="2" Classes="StatusPanelButton" Background="{DynamicResource ThemeControlLowBrush}" Margin="5" Padding="7" HorizontalAlignment="Stretch"
                        VerticalAlignment="Stretch" CornerRadius="5" MinWidth="50" MinHeight="40"
                        Name="SettingsButton"
                        Click="SettingsButton_Click">
                    <ToolTip.Tip>
                        <Label Content="{x:Static openSshGui:StringsAndTexts.MainWindowRightClickOptions}" />
                    </ToolTip.Tip>
                    <Button.ContextMenu>
                        <ContextMenu PlacementAnchor="Top" Placement="Top" WindowManagerAddShadowHint="True">
                            <MenuItem
                                Header="{x:Static openSshGui:StringsAndTexts.MainWindowSettingsMenuReloadKeysFromDiskAndForgetPasswords}"
                                Command="{Binding ReloadKeys}">
                                <MenuItem.CommandParameter>
                                    <sys:Boolean>True</sys:Boolean>
                                </MenuItem.CommandParameter>
                                <MenuItem.Icon>
                                    <materialIcons:MaterialIcon Kind="Reload" />
                                </MenuItem.Icon>
                            </MenuItem>
                            <MenuItem
                                Header="{x:Static openSshGui:StringsAndTexts.MainWindowSettingsMenuReloadKeysFromDisk}"
                                Command="{Binding ReloadKeys}">
                                <MenuItem.CommandParameter>
                                    <sys:Boolean>False</sys:Boolean>
                                </MenuItem.CommandParameter>
                                <MenuItem.Icon>
                                    <materialIcons:MaterialIcon Kind="Reload" />
                                </MenuItem.Icon>
                            </MenuItem>
                            <Separator />
                            <MenuItem Header="{x:Static openSshGui:StringsAndTexts.MainWindowSettingsMenuAppSettings}"
                                      Command="{Binding OpenAppSettings}">
                                <MenuItem.Icon>
                                    <materialIcons:MaterialIcon Kind="ApplicationSettingsOutline" />
                                </MenuItem.Icon>
                            </MenuItem>
                            <MenuItem
                                Header="{x:Static openSshGui:StringsAndTexts.MainWindowSettingsMenuLocalUserSshConfig}"
                                Command="{Binding NotImplementedMessage}">
                                <MenuItem.Icon>
                                    <materialIcons:MaterialIcon Kind="AccountCogOutline" />
                                </MenuItem.Icon>
                            </MenuItem>
                            <MenuItem
                                Header="{x:Static openSshGui:StringsAndTexts.MainWindowSettingsMenuMachineSshdConfig}"
                                Command="{Binding NotImplementedMessage}">
                                <MenuItem.Icon>
                                    <materialIcons:MaterialIcon Kind="BookCogOutline" />
                                </MenuItem.Icon>
                            </MenuItem>
                            <Separator />
                            <MenuItem
                                Header="{x:Static openSshGui:StringsAndTexts.MainWindowSettingsMenuRemoteUserSshConfig}"
                                IsEnabled="{Binding ServerConnection.IsConnected}"
                                Command="{Binding NotImplementedMessage}">
                                <MenuItem.Icon>
                                    <materialIcons:MaterialIcon Kind="ServerNetworkOutline" />
                                </MenuItem.Icon>
                            </MenuItem>
                            <MenuItem
                                Header="{x:Static openSshGui:StringsAndTexts.MainWindowSettingsMenuRemoteMachineSshdConfig}"
                                IsEnabled="{Binding ServerConnection.IsConnected}"
                                Command="{Binding NotImplementedMessage}">
                                <MenuItem.Icon>
                                    <materialIcons:MaterialIcon Kind="ServerOutline" />
                                </MenuItem.Icon>
                            </MenuItem>
                            <MenuItem Header="{Binding Version}" IsEnabled="False">
                                <MenuItem.Icon>
                                    <materialIcons:MaterialIcon Kind="LocalOffer"/>
                                </MenuItem.Icon>
                            </MenuItem>
                        </ContextMenu>
                    </Button.ContextMenu>
                    <materialIcons:MaterialIcon Kind="CogOutline" />
                </Button>

                <!-- Help -->

                <Button Grid.Column="3" Classes="StatusPanelButton" Background="{DynamicResource ThemeControlLowBrush}" Margin="5" Padding="7" HorizontalAlignment="Stretch"
                        VerticalAlignment="Stretch" CornerRadius="5" MinWidth="50" MinHeight="40"
                        Name="HelpButton"
                        Click="HelpButton_Click">
                    <ToolTip.Tip>
                        <Label Content="{x:Static openSshGui:StringsAndTexts.MainWindowRightClickHelp}" />
                    </ToolTip.Tip>
                    <Button.ContextMenu>
                        <ContextMenu PlacementAnchor="Top" Placement="Top" WindowManagerAddShadowHint="True">
                            <MenuItem Header="{x:Static openSshGui:StringsAndTexts.MainWindowHelpMenuWebsite}"
                                      Command="{Binding OpenBrowser}">
                                <MenuItem.CommandParameter>
                                    <sys:Int32>0</sys:Int32>
                                </MenuItem.CommandParameter>
                                <MenuItem.Icon>
                                    <materialIcons:MaterialIcon Kind="Web" />
                                </MenuItem.Icon>
                            </MenuItem>
                            <Separator />
                            <MenuItem Header="{x:Static openSshGui:StringsAndTexts.MainWindowHelpMenuQuestion}"
                                      Command="{Binding OpenBrowser}">
                                <MenuItem.CommandParameter>
                                    <sys:Int32>1</sys:Int32>
                                </MenuItem.CommandParameter>
                                <MenuItem.Icon>
                                    <materialIcons:MaterialIcon Kind="LightbulbQuestionOutline" />
                                </MenuItem.Icon>
                            </MenuItem>
                            <MenuItem Header="{x:Static openSshGui:StringsAndTexts.MainWindowHelpMenuAbout}"
                                      Command="{Binding OpenBrowser}">
                                <MenuItem.CommandParameter>
                                    <sys:Int32>2</sys:Int32>
                                </MenuItem.CommandParameter>
                                <MenuItem.Icon>
                                    <materialIcons:MaterialIcon Kind="Help" />
                                </MenuItem.Icon>
                            </MenuItem>
                        </ContextMenu>
                    </Button.ContextMenu>
                    <materialIcons:MaterialIcon Kind="HelpCircleOutline" />
                </Button>
            </Grid>
        </Border>
        </Grid>
    </Grid>
</Window>